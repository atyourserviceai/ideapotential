{
  "name": "ideapotential",
  "version": "1.0.0",
  "description": "AI-powered startup idea validation tool with 10-factor assessment",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "react-router dev --port 5773",
    "build": "react-router build",
    "preview": "vite preview --port 5773",
    "deploy": "react-router build && wrangler deploy",
    "deploy:staging": "CLOUDFLARE_ENV=staging pnpm run deploy",
    "deploy:production": "CLOUDFLARE_ENV=production pnpm run deploy",
    "logs:deployed": "wrangler tail",
    "logs:staging": "wrangler tail --env staging",
    "test": "vitest",
    "types": "wrangler types",
    "format": "prettier --write .",
    "check": "prettier . --check && biome lint && tsc"
  },
  "keywords": [
    "startup",
    "validation",
    "assessment",
    "ai",
    "agents",
    "cloudflare",
    "ideapotential"
  ],
  "author": "AtYourService.ai",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@cloudflare/puppeteer": "^1.0.3",
    "@cloudflare/vite-plugin": "1.7.4",
    "@cloudflare/vitest-pool-workers": "^0.8.43",
    "@cloudflare/workers-types": "^4.20250620.0",
    "@react-router/dev": "^7.8.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "htm": "^3.1.1",
    "prettier": "^3.5.3",
    "react-router": "^7.8.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "3.2.4",
    "wrangler": "^4.20.5"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@types/marked": "^6.0.0",
    "agents": "^0.0.97",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "composio-core": "^0.5.39",
    "html2canvas": "^1.4.1",
    "jsdom": "^26.1.0",
    "jspdf": "^2.5.2",
    "marked": "^15.0.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  }
}
